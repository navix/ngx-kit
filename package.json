{
  "name": "ngx-kit-src",
  "version": "8.0.0-beta.6",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf ./dist && rimraf ./dist-server && rimraf ./covarage && rimraf ./tmp",
    "evo:build": "ng run evo:build",
    "evo:pack": "npm run release:cli -- --package=evo --pack",
    "evo:release": "npm run release:cli -- --package=evo --publish",
    "playground": "ng serve --project=playground",
    "playground:hmr": "ng run playground:serve:hmr",
    "schematics:build": "tsc -p ./packages/schematics/tsconfig.json && node ./release-schematics",
    "website:serve": "ng serve --project=website",
    "website:docgen": "npm run website:docgen:core && npm run website:docgen:collection",
    "website:docgen:core": "node ./dist/docgen/cli -p ./packages/core/ -o ./website/app/content/core.json",
    "website:docgen:collection": "node ./dist/docgen/cli -p ./packages/collection/ -o ./website/app/content/collection.json",
    "website:build": "ng run website:build:production",
    "website:server:build": "ng run website:server:production",
    "website:server:bundle": "webpack --config ./website/webpack.server.config.js --progress --colors",
    "prewebsite:server:serve": "npm run website:build && npm run website:server:build && npm run website:server:bundle",
    "website:server:serve": "node ./dist/server",
    "website:deploy": "npm run clean && npm run docgen:build:tsc && npm run website:docgen && npm run website:build && npm run website:server:build && npm run website:server:bundle && npm run website:docker:build && npm run website:docker:push",
    "website:docker:build": "docker build -t registry.gitlab.com/navix/dhub/ngx-kit/website .",
    "website:docker:run": "docker run -d -p 8002:8000 registry.gitlab.com/navix/dhub/ngx-kit/website",
    "website:docker:push": "docker push registry.gitlab.com/navix/dhub/ngx-kit/website",
    "release:cli": "ts-node --project tsconfig.cli.json release.ts",
    "test": "ng test --watch=false",
    "test:watch": "ng test --watch=true",
    "lint": "npm run lint:ng && npm run lint:styles",
    "lint:ng": "ng lint --project=website",
    "lint:styles": "stylelint ./{packages/collection,playground,website}/**/*.scss",
    "lint:styles:fix": "stylelint ./{packages/collection,playground,website}/**/*.scss --fix",
    "docgen:build": "npm run docgen:build:tsc && npm run docgen:build:release && npm run docgen:build:publish",
    "docgen:build:tsc": "tsc -p ./packages/docgen/tsconfig.json",
    "docgen:build:release": "node ./packages/docgen/release.js",
    "docgen:build:publish": "dev-publish.sh -p docgen -w"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/cdk": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/router": "^8.0.0",
    "@angular/service-worker": "^8.0.0",
    "@nvxme/monit-ng-client": "^1.0.1",
    "@nvxme/ngx-md-render": "^1.0.0-alpha.5",
    "hammerjs": "^2.0.8",
    "moment": "^2.22.2",
    "rxjs": "^6.5.2",
    "tsickle": "^0.35.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular-devkit/build-ng-packagr": "~0.800.0",
    "@angular-devkit/core": "^8.0.0",
    "@angular-devkit/schematics": "^8.0.0",
    "@angular/cli": "^8.0.0",
    "@angular/platform-server": "^8.0.0",
    "@angularclass/hmr": "^2.1.3",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "^7.1.1",
    "@ngx-kit/schematics": "^2.1.0-beta.2",
    "@nvxme/monit-client": "^2.0.1",
    "@types/express": "^4.11.1",
    "@types/fs-extra": "^7.0.0",
    "@types/glob": "^7.1.1",
    "@types/hammerjs": "^2.0.36",
    "@types/highlight.js": "^9.12.2",
    "@types/jasmine": "^3.3.13",
    "@types/node": "^12.0.3",
    "@types/shelljs": "^0.8.5",
    "@types/yargs": "^13.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "chalk": "^2.4.2",
    "codelyzer": "^5.0.1",
    "core-js": "^3.1.3",
    "express": "^4.17.1",
    "front-matter": "^3.0.2",
    "fs-extra": "^8.0.1",
    "glob": "^7.1.4",
    "jasmine": "^3.4.0",
    "jasmine-core": "^3.4.0",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.4",
    "karma-jasmine": "^2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "lerna": "^3.14.1",
    "ncp": "^2.0.0",
    "ng-packagr": "^5.2.0",
    "rimraf": "^2.6.2",
    "rollup": "^1.12.4",
    "shelljs": "^0.8.3",
    "stylelint": "^10.0.1",
    "stylelint-order": "^3.0.0",
    "ts-node": "^8.2.0",
    "tslint": "^5.16.0",
    "typescript": "3.4.5",
    "walk": "^2.3.14",
    "webpack-cli": "^3.3.2",
    "yargs": "^13.2.4",
    "yargs-parser": "^13.1.0"
  }
}
