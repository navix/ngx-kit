    "website": {
      "root": "",
      "sourceRoot": "website",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/website",
            "index": "website/index.html",
            "main": "website/main.ts",
            "tsConfig": "website/tsconfig.app.json",
            "polyfills": "website/polyfills.ts",
            "assets": [
              "website/favicon.ico",
              "website/robots.txt",
              "website/assets"
            ],
            "styles": [
              "website/styles/main.scss",
              "website/assets/hljs-default.scss"
            ],
            "scripts": [
              "node_modules/hammerjs/hammer.js"
            ],
            "stylePreprocessorOptions": {
              "includePaths": [
                "website/styles"
              ]
            }
          },
          "configurations": {
            "production": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "website/environments/environment.ts",
                  "with": "website/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "website:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "website/tsconfig.app.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist-server/",
            "main": "website/main.server.ts",
            "tsConfig": "website/tsconfig.server.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "website/styles"
              ]
            }
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "website/environments/environment.ts",
                  "with": "website/environments/environment.prod.ts"
                }
              ]
            }
          }
        }
      }
    }
